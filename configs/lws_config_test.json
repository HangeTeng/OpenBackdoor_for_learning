{
    "target_dataset":{
        "name": "sst-2",
        "dev_rate": 0.1
    },
    "poison_dataset":{
        "name": "sst-2",
        "dev_rate": 0.1
    },
    "victim":{
        "type": "plm",
        "model": "bert",
        "path": "bert-base-uncased",
        "num_classes": 2,
        "device": "gpu",
        "max_len": 512
    },
    "attacker":{
        "name": "lws",
        "metrics": ["accuracy"],
        "train":{
            "name": "lws",
            "lr": 2e-5,
            "weight_decay": 0,
            "epochs": 2,
            "batch_size": 32,
            "warm_up_epochs": 3,
            "ckpt": "best",
            "save_path": "./models"
        }
    },

    "clean-tune": false

}